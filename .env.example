# Environment variables required for build/runtime

# Google Gemini API (required)
VITE_GEMINI_API_KEY="your-gemini-api-key-here"

# Supabase (optional)
VITE_SUPABASE_URL="https://xyz.supabase.co"
VITE_SUPABASE_ANON_KEY="your-supabase-anon-key"

# Optional feature flags (default-off)
# Set to true to enable enhancements; otherwise omit or set false
VITE_FEATURE_LOGGING=false
VITE_FEATURE_CACHE=false
VITE_FEATURE_JOBQUEUE=false
VITE_FEATURE_PLUGINS=false
VITE_FEATURE_ANALYTICS=false
VITE_FEATURE_LAZY=false